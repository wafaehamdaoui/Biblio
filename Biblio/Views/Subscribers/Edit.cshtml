@model Biblio.Models.Subscriber

@{
    ViewData["Title"] = "Edit";
}

<h4 class="text-info mt-5"> Edit Student</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Id" />
            <div class="form-group mt-3">
                <label asp-for="Id" class="control-label ms-1"></label>
                <input asp-for="Id" disabled class="form-control" />
                <span asp-validation-for="Id" class="text-danger"></span>
            </div>
            <div class="form-group mt-3">
                <label asp-for="FirstName" class="control-label ms-1"></label>
                <input asp-for="FirstName" class="form-control" />
                <span asp-validation-for="FirstName" class="text-danger"></span>
            </div>
            <div class="form-group mt-3">
                <label asp-for="LastName" class="control-label ms-1"></label>
                <input asp-for="LastName" class="form-control" />
                <span asp-validation-for="LastName" class="text-danger"></span>
            </div>
            <div class="form-group mt-3">
                <input type="submit" value="Save" class="btn btn-primary ms-1" />
                <a asp-controller="Subscribers" asp-action="Index" class="btn btn-light">Back to List</a>
            </div>
        </form>
    </div>
</div>


